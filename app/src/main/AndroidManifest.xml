<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bluetooth.admin.balencecar">

    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.CHANGE_CONFIGURATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name="com.bluetooth.admin.balencecar.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainInterface"></activity>
        <activity android:name=".Task_1"></activity>
        <activity android:name=".Task_2"></activity>
        <activity android:name=".Task_3"></activity>
        <activity android:name=".Task_4"></activity>
        <activity android:name=".Task_5"></activity>
        <activity android:name=".Task_6"></activity>
        <activity android:name=".Task_7"></activity>
        <activity android:name=".Task_8"></activity>
        <activity android:name=".Task_chukong"></activity>
        <activity android:name=".Task_jiguang"></activity>
        <service
            android:name="com.bluetooth.admin.balencecar.MyService"
            android:enabled="true"
            android:exported="true">
        </service>
    </application>

</manifest>